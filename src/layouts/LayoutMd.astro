---
import Layout from './Layout.astro';
const { frontmatter } = Astro.props;
---

<Layout title={frontmatter.title}>
  <div class="flex justify-center pt-20 pb-32 px-6">
    <article class="prose prose-sm w-full max-w-6xl px-14 prose-headings:font-larken">
      <slot />
    </article>
  </div>
</Layout>
